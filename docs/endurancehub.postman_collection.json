          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Status 200', function () { pm.response.to.have.status(200); });",
                  "const body = pm.response.json();",
                  "const runner = body.athletes.find((athlete) => athlete.user && athlete.user.email === 'runner@endurancehub.test');",
                  "const triathlete = body.athletes.find((athlete) => athlete.user && athlete.user.email === 'triathlete@endurancehub.test');",
                  "const tempEmail = pm.collectionVariables.get('tempAthleteEmail');",
                  "const tempAthlete = tempEmail ? body.athletes.find((athlete) => athlete.user && athlete.user.email === tempEmail) : null;",
                  "pm.collectionVariables.set('runnerAthleteId', runner ? String(runner.id) : '');",
                  "pm.collectionVariables.set('triathleteAthleteId', triathlete ? String(triathlete.id) : '');",
                  "if (tempAthlete) { pm.collectionVariables.set('tempAthleteId', String(tempAthlete.id)); }",
                  "pm.test('Runner athlete present', function () { pm.expect(runner).to.be.an('object'); });",
                  "if (tempEmail) { pm.test('Temp athlete located', function () { pm.expect(tempAthlete).to.be.an('object'); }); }"
                ],
                "type": "text/javascript"
              }
            }
          ]
